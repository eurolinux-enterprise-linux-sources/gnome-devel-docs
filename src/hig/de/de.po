#: C/hig-ch-menus.xml:1225(row/entry)
msgid "<guimenuitem><accel>L</accel>ist...</guimenuitem>"
msgstr "<guimenuitem><accel>L</accel>ist...</guimenuitem>"

#: C/index.docbook:1227(row/entry)
#: C/hig-ch-menus.xml:1227(row/entry)
msgid "Sets the properties of the selected list, or turns the selected paragraphs into a list if they are not already formatted as such. Open a dialog allowing the user to choose number or bullet style, spacing, tabulation, or whatever other attributes are applicable."
msgstr ""

#: C/index.docbook:1232(row/entry)
#: C/hig-ch-menus.xml:1232(row/entry)
msgid "<guimenuitem>Laye<accel>r</accel>...</guimenuitem>"
msgstr "<guimenuitem>Laye<accel>r</accel>...</guimenuitem>"

